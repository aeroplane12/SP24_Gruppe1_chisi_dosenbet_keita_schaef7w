<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollBar?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="460.0" prefWidth="740.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sw.praktikum.spinfood.Controller">
   <children>
      <HBox layoutX="14.0" layoutY="14.0" prefHeight="25.0" prefWidth="335.0">
         <children>
            <Button fx:id="uploadCSV" mnemonicParsing="false" onAction="#handleUploadCSV" prefHeight="25.0" prefWidth="94.0" text="Upload CSV" />
            <Button fx:id="location" mnemonicParsing="false" onAction="#handleLocation" prefHeight="25.0" prefWidth="114.0" text="Upload Location" />
            <Button fx:id="calculate" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#handleCalculate" prefHeight="25.0" prefWidth="134.0" text="Calculate" />
