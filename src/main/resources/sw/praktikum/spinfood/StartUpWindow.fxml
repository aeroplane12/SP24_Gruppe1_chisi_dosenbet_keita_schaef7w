                        <TableView prefHeight="303.0" prefWidth="706.0">
                           <columns>
                              <TableColumn prefWidth="75.0" text="ID" />
                              <TableColumn prefWidth="75.0" text="Name" />
                              <TableColumn prefWidth="104.0" text="Food Preference" />
