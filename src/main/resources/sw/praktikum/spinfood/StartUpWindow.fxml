<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="364.0" prefWidth="451.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sw.praktikum.spinfood.Controller">
   <children>
      <HBox layoutX="14.0" layoutY="14.0" prefHeight="25.0" prefWidth="254.0">
         <children>
            <Button fx:id="uploadCSV" mnemonicParsing="false" onAction="#handleUploadCSV" prefHeight="25.0" prefWidth="94.0" text="Upload CSV" />
            <Button fx:id="location" mnemonicParsing="false" onAction="#handleLocation" prefHeight="25.0" prefWidth="90.0" text="Location" />
            <Button fx:id="calculate" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#handleCalculate" prefHeight="25.0" prefWidth="104.0" text="Calculate" />
         </children>
      </HBox>
      <HBox layoutX="14.0" layoutY="56.0" prefHeight="25.0" prefWidth="254.0">
         <children>
            <Button fx:id="personTab" mnemonicParsing="false" onAction="#handlePersonTab" prefHeight="25.0" prefWidth="106.0" text="Person" />
            <Button fx:id="coupleTab" mnemonicParsing="false" onAction="#handleCoupleTab" prefHeight="25.0" prefWidth="89.0" text="Couple" />
            <Button fx:id="groupTab" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#handleGroupTab" prefHeight="25.0" prefWidth="125.0" text="Group" />
         </children>
      </HBox>
      <ComboBox layoutX="14.0" layoutY="100.0" prefHeight="25.0" prefWidth="262.0" />
      <ListView id="listView" fx:id="listField" layoutX="15.0" layoutY="138.0" prefHeight="212.0" prefWidth="421.0" />
      <Button fx:id="undo" layoutX="351.0" layoutY="56.0" mnemonicParsing="false" onAction="#handleUndo" text="&lt;--" />
      <Button fx:id="redo" layoutX="391.0" layoutY="56.0" mnemonicParsing="false" onAction="#handleRedo" text="--&gt;" />
      <Button fx:id="languageGerman" layoutX="350.0" layoutY="15.0" mnemonicParsing="false" onAction="#handleLanguageGerman" prefHeight="25.0" prefWidth="34.0" text="DE" />
      <Button fx:id="languageEnglish" layoutX="392.0" layoutY="15.0" mnemonicParsing="false" onAction="#handleLanguageEnglish" prefHeight="25.0" prefWidth="34.0" text="EN" />
      <Button layoutX="398.0" layoutY="100.0" mnemonicParsing="false" onAction="#handleOpenWindow" text="*" />
   </children>
</AnchorPane>
